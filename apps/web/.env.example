# Environment variables for the AI Front Desk demo

# OpenAI key used on the server to mint realtime sessions.
OPENAI_API_KEY=

# Secret key for the Stripe API.  Leave blank for demo mode.
STRIPE_SECRET=

# After you configure the webhook in your Stripe dashboard, add the signing secret here.
STRIPE_WEBHOOK_SECRET=

# The base URL of your deployed site, e.g. https://ai-frontdesk-simple.netlify.app
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Deposit configuration
DEPOSIT_AMOUNT_CENTS=4900
DEPOSIT_CURRENCY=usd
